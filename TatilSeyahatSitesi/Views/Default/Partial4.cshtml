@model List<TatilSeyahatSitesi.Models.Siniflar.blog>
<div class="padding1">
    <h4>En Beğendiğim Yerler </h4>
    <div class="box-left">
        @foreach (var x in Model)
        {


        <div class="bot-img">
            <img src="@x.blogImage" style="width:120px; height:65px;" alt="" />
        </div>
        <div class="bot-desc">
            <p>@x.baciklama.Substring(0,75).. </p>
            <a href="/Blog/BlogDetails/@x.blogID" class="btn btn-group">Bloga Git</a>
        </div>
        <div class="clear"></div>
       
        }
    </div>
    @Html.Action("Partial5","Default")
</div>
